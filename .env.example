APP_NAME="Roble Tailwind"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=America/Caracas
APP_URL=http://localhost

APP_LOCALE=es_VE
APP_FALLBACK_LOCALE=es
APP_FAKER_LOCALE=es_VE

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=pgsql
DB_PORT=5432
DB_DATABASE=roble
DB_USERNAME=sail
DB_PASSWORD=password

DB_HOST_ORG=pgsql
DB_PORT_ORG=5432
DB_DATABASE_ORG=empresa
DB_USERNAME_ORG=sail
DB_PASSWORD_ORG=password

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=reverb
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_SCHEME=null
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# see https://laravel.com/docs/12.x/reverb#main-content
# YOU MUST SET these variables in order to broadcasted notifications work properly
# must be an unique string with at least 6 characters length,  see https://www.random.org/integers
REVERB_APP_ID=
# must be an unique string with at least 20 characters length, see https://www.random.org/strings
REVERB_APP_KEY=
# must be an unique string with at least 20 characters length, see https://www.random.org/strings
REVERB_APP_SECRET=
REVERB_HOST="localhost"
REVERB_PORT=8080
REVERB_SCHEME=http

VITE_REVERB_APP_KEY="${REVERB_APP_KEY}"
VITE_REVERB_HOST="${REVERB_HOST}"
VITE_REVERB_PORT="${REVERB_PORT}"
VITE_REVERB_SCHEME="${REVERB_SCHEME}"
